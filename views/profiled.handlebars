<div class="wrapper-main">
    <div class="wrapper-header">
        <h2 id="profileHeader">Welcome to your profile page</h2>
    </div>


    {{!-- <body> --}}
    <div class="wrapper-main">

        <!-- Home Section -->
        <section id="home">
            <div class="jumbotron jumbotron2">
                <div class="container">
                    <h2 id="savedCharitiesHeader">Your Saved Charities</h2>
                    <div class="card-group">
                        {{#each savedCharities}}
                        <div class="card" id="saved-charities-block">
                            <div class="card-header">
                                {{charityName}}
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-lg-9">
                                        <h5 class="card-title">{{tagLine}}</h5>
                                    </div>
                                    <div class="col-lg-3">
                                        <a class="more-info" href="/charity/{{ein}}" data-analytics-subtype="button"
                                            data-analytics-eventvalue="Get More Information"
                                            data-analytics-pagesection="categories" id="{{ein}}">
                                            <div id="info-button">
                                                <i class="fa fa-info"> Click for Information</i>
                                            </div>
                                        </a>
                                    </div>

                                </div>
                                <p class="card-text">Mission: {{mission}}</p>
                                <p class="card-text">Current Rating: {{currentRating}} <img src={{currentRatingImg}}
                                        alt="..."></p>

                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>


            </div>
            <h1 class="browse-fdr-category-header dna-h2">Your Saved Charity Categories</h1>
            <br>
            {{#each savedCategories}}
            <a class="dna-grid-col-l-3 category-wrapper fundingTag" href="/category/{{Category.categoryID}}"
                data-analytics-subtype="button" data-analytics-eventvalue={{category}}
                data-analytics-pagesection="categories" id={{Category.categoryID}}>
                <i class="{{Category.iconName}}"> {{category}}</i>
            </a>
            {{/each}}
            <div class="browse-fdr-category">
                <div class="dna-grid-container">
                    <section class="dna-grid-row categories">
                </div>
                <br>
                <br>
                <section class=" dna-grid-row">
                    <div class="dna-grid-col-s-12 dna-grid-col-m-6 dna-grid-col-l-12 dna-grid-row category-container">
                    </div>

            </div>
        </section>

        <h2>Top Rated Charities</h2>

        <div class="container">
            <div class="row styled-row">
                {{#each topRatedCharities}}
                <div class="card prof-card" id="charity-cards">
                    <div class="card-title">{{name}}</div>
                        <p class="card-text-margin">{{tagLine}}</p>
                        <p class="card-text-margin">Current Rating: {{currentRating}} /4 <img class="stars"
                                src={{currentRatingImg}} alt="...">
                        </p>
                        <a class="more-info card-text-margin" href="/charity/{{ein}}" data-analytics-subtype="button"
                            data-analytics-eventvalue="Get More Information" data-analytics-pagesection="categories"
                            id="{{ein}}">more info
                            <i class="fa fa-info-circle fa-style" aria-hidden="true"></i>
                        </a>
                        <p class="card-text-margin">Current Rating: {{currentRating}} <img src={{currentRatingImg}} alt="..."></p>
                    </div>
                
                {{/each}}
            </div>
        </div>

